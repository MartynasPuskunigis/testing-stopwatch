{
  "extends": "simplr-tslint",
  "rules": {
      "class-members-name": [
          true,
          {
              "formatRules": [{ "kind": "property", "format": "camel-case", "allowedPrefixes": ["_", "$"] }],
              "ignoreParentSuffixes": ["Dto"]
          }
      ],
      "import-components": true,
      "import-module": true
  }
}
